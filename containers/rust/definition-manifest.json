{
	"definitionVersion": "0.200.3",
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"rust:${VERSION}-1"
		]
	},
	"dependencies": {
		"image": "rust:1",
		"imageLink": "https://hub.docker.com/_/rust",
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh"
		},
		"cargo": {
			"rls": null,
			"rustfmt": null,
			"rust-analysis": "rustc --version",
			"rust-src": "rustc --version",
			"clippy": "rustc --version"
		},
		"languages": {
			"Rust": {
				"versionCommand": "rustc --version | grep -oE '^rustc\\s[^ ]+' | sed -n '/rustc\\s/s///p'",
				"path": "/usr/local/cargo",
				"downloadUrl": "https://github.com/rust-lang/rust"
			}
		}
	}
}
