{
	"definitionVersion": "1.3.2",
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"universal:${VERSION}-focal",
			"universal:${VERSION}-linux",
			"universal:${VERSION}"
		]
	},
	"dependencies": {
		"image": "mcr.microsoft.com/oryx/build:vso-focal-20210308.3",
		"imageLink": "https://github.com/microsoft/oryx",
		"apt": [{
				"cgIgnore": false,
				"name": "moby-cli",
				"annotation": "Docker CLI"
			}, {
				"cgIgnore": false,
				"name": "moby-engine",
				"annotation": "Docker Engine"
			}, {
				"cgIgnore": false,
				"name": "git-lfs",
				"annotation": "Git Large File Support"
			}, {
				"cgIgnore": false,
				"markdownIgnore": true,
				"name": "powershell"
			}, {
				"cgIgnore": false,
				"name": "azure-cli",
				"annotation": "Azure CLI"
			},
			"cmake",
			"build-essential", 
			"cmake",
			"cppcheck",
			"valgrind",
			"lldb",
			"llvm", 
			"gdb",
			"clang",
			"python3-dev",
			"vim",
			"vim-doc",
			"xtail",
			"software-properties-common",
			"libsecret-1-dev",
			"libnss3", 
			"libnspr4",
			"libatk-bridge2.0-0",
			"libatk1.0-0",
			"libx11-6",
			"libpangocairo-1.0-0",
			"libx11-xcb1",
			"libcups2",
			"libxcomposite1",
			"libxdamage1",
			"libxfixes3",
			"libpango-1.0-0",
			"libgbm1",
			"libgtk-3-0",
			"openssh-server",
			"lxc",
			"pigz",
			"iptables",
			"tar",
			"g++",
			"gcc",
			"libc6-dev",
			"make", 
			"pkg-config",
			"sed",
			"python3-minimal"
		],
		"pipx": [
			"pylint",
			"flake8",
			"autopep8",
			"black",
			"yapf",
			"mypy",
			"pydocstyle",
			"pycodestyle",
			"bandit",
			"virtualenv",
			"pipx"
		],
		"git": {
			"Oh My Zsh!": "/home/codespace/.oh-my-zsh",
			"nvm": "/home/codespace/.nvm",
			"nvs": "/home/codespace/.nvs",
			"rbenv": "/usr/local/share/rbenv",
			"ruby-build": "/usr/local/share/ruby-build"
		},
		"gem": [
			"rake",
			"ruby-debug-ide",
			"debase"
		],
		"cargo": {
			"rls": null,
			"rustfmt": null,
			"rust-analysis": "rustc --version",
			"rust-src": "rustc --version",
			"clippy": "rustc --version"
		},
		"go": {
			"golang.org/x/tools/gopls": null,
			"honnef.co/go/tools": null,
			"golang.org/x/lint": null,
			"github.com/mgechev/revive": null,
			"github.com/uudashr/gopkgs": null,
			"github.com/ramya-rao-a/go-outline": null,
			"github.com/go-delve/delve": null,
			"github.com/golangci/golangci-lint": null
		},
		"other": {
			"git": {
				"versionCommand": "git --version | sed -n '/git version /s///p'",
				"downloadUrl": "https://github.com/git/git"
			},
			"Xdebug": {
				"versionCommand": "php --version | grep -oE 'with Xdebug\\sv[^,]+'| sed -n '/with Xdebug\\sv/s///p'",
				"downloadUrl": "https://xdebug.org/"
			},
			"Composer": {
				"cgIgnore": "true",
				"versionCommand": "composer --no-ansi --version 2>/dev/null | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+'",
				"downloadUrl": "https://getcomposer.org/"
			},
			"kubectl": {
				"versionCommand": "kubectl version --client | grep -oE 'GitVersion:\\\"v[^\\\"]+' | sed -n '/GitVersion:\\\"v/s///p'",
				"downloadUrl": "https://github.com/kubernetes/kubectl"
			},
			"Helm": {
				"versionCommand": "helm version | grep -oE 'Version:\\\"v[^\\\"]+' | sed -n '/Version:\\\"v/s///p'",
				"downloadUrl": "https://github.com/helm/helm"
			},
			"Docker Compose": {
				"versionCommand": "docker-compose version | grep -m 1 -oE '[0-9]+\\.[0-9]+\\.[0-9]+[+,0-9,a-z]*'",
				"downloadUrl": "https://github.com/docker/compose"
			},
			"SDKMAN!": {
				"versionCommand": ". /usr/local/sdkman/bin/sdkman-init.sh && sdk version | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+[+,0-9,a-z]*' | tr -d '[:cntrl:]'",
				"downloadUrl": "https://github.com/sdkman/sdkman-cli"
			},
			"rvm": {
				"versionCommand": ". /usr/local/rvm/scripts/rvm > /dev/null 2>&1 && rvm --version | grep -oE -m 1 'rvm\\s[^ ]+' | sed -n '/^rvm\\s/s///p'",
				"path": "/usr/local/rvm",
				"downloadUrl": "https://github.com/rvm/rvm"
			},
			"AdoptOpenJDK": {
				"markdownIgnore": true,
				"versionCommand": "/usr/local/sdkman/candidates/java/8*/bin/java -version 2>&1 | grep -oE -m 1 '.*build\\s[^\\)]+' | sed -n '/.*build\\s/s///p'",
				"downloadUrl": "https://adoptopenjdk.net/"
			},
			"GitHub CLI": {
				"versionCommand": "gh version 2>&1 | grep -oE 'version\\s[^ ]+' | sed -n '/version\\s/s///p'",
				"downloadUrl": "https://github.com/cli/cli"
			},
			"yarn": {
				"cgIgnore": true,
				"versionCommand": "yarn --version",
				"downloadUrl": "https://yarnpkg.com/",
				"path": "/opt/yarn"
			},
			"Maven": {
				"cgIgnore": true,
				"versionCommand": "mvn -version | grep -oE '\\\"[0-9]+\\.[0-9]\\.[0-9][^\\\"]+' | tr -d '\\\"'",
				"path": "/opt/maven",
				"downloadUrl": "https://maven.apache.org/"
			},
			"Gradle": {
				"versionCommand": ". /usr/local/sdkman/bin/sdkman-init.sh && gradle --version | grep -ozP 'Gradle\\s+\\K[0-9]\\.[0-9]\\.[0-9]' | tr -d '\\0'",
				"downloadUrl": "https://gradle.org/"
			},
			"Docker (Moby) CLI & Engine": {
				"cgIgnore": true,
				"versionCommand": "docker --version | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+[+0-9a-z]*'"
			}
		},
		"languages": {
			"Node.js": {
				"cgIgnore": true,
				"versionCommand": "ls /opt/nodejs | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+'",
				"path": "/opt/nodejs/&lt;version&gt;",
				"downloadUrl": "https://nodejs.org/en/"
			},
			"Python": {
				"cgIgnore": true,
				"versionCommand": "ls /opt/python | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+'",
				"path": "/opt/python/&lt;version&gt;",
				"downloadUrl": "https://www.python.org/"
			},
			"Java": {
				"cgIgnore": true,
				"versionCommand": "ls /opt/java | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+'",
				"path": "/opt/java/&lt;version&gt;",
				"downloadUrl": "https://adoptopenjdk.net/"
			},
			".NET": {
				"cgIgnore": true,
				"versionCommand": "ls /opt/dotnet | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+'",
				"path": "/home/codespaces/.dotnet",
				"downloadUrl": "https://dotnet.microsoft.com/"
			},
			"Ruby": {
				"cgIgnore": true,
				"versionCommand": "ls /opt/ruby | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+'",
				"path": "/opt/ruby/&lt;version&gt;",
				"downloadUrl": "https://www.ruby-lang.org/en/"
			},
			"PHP": {
				"cgIgnore": true,
				"versionCommand": "ls /opt/php | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+'",
				"path": "/opt/php/&lt;version&gt;",
				"downloadUrl": "https://www.php.net/"
			},
			"PowerShell": {
				"cgIgnore": true,
				"versionCommand": "pwsh --version | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+'",
				"path": "/opt/microsoft/powershell",
				"downloadUrl": "https://docs.microsoft.com/en-us/powershell/"
			},
			"GCC": {
				"cgIgnore": true,
				"versionCommand": "gcc --version | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+.+)' | tr -d ')'"
			},
			"Clang": {
				"cgIgnore": true,
				"versionCommand": "clang --version | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+.*'"
			},
			"Go": {
				"versionCommand": "go version | grep -oE -m 1 'version\\sgo[^ ]+' | sed -n '/^version\\sgo/s///p'",
				"path": "/usr/local/go",
				"downloadUrl": "https://golang.org/dl"
			},
			"Rust": {
				"versionCommand": "rustc --version | grep -oE '^rustc\\s[^ ]+' | sed -n '/rustc\\s/s///p'",
				"path": "/usr/local/cargo",
				"downloadUrl": "https://github.com/rust-lang/rust"
			}
		}
	}
}