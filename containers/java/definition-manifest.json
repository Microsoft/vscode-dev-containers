{
	"variants": [ "15", "11" ],
	"definitionVersion": "0.201.4",
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"java:${VERSION}-${VARIANT}"
		]
	},
	"dependencies": {
		"image": "openjdk:${VARIANT}-jdk-buster",
		"imageLink": "https://hub.docker.com/_/java",
		"apt": [{
			"cgIgnore": false,
			"name": "yarn"
		}],
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh",
			"nvm": "/usr/local/share/nvm"
		},
		"other": {
			"SDKMAN!": {
				"versionCommand": ". /usr/local/sdkman/bin/sdkman-init.sh && sdk version | grep -ozP 'SDKMAN\\s+\\K[0-9]+\\.[0-9]+\\.[0-9]+[+,0-9,a-z]*' | tr -d '[:cntrl:]'",
				"path": "/usr/local/sdkman",
				"downloadUrl": "https://github.com/sdkman/sdkman-cli"
			}
		},
		"languages": {
			"Java": {
				"cgIgnore": true,
				"versionCommand": "java --version | grep -ozP 'openjdk\\s+\\K[0-9]+\\.[0-9]+\\.[0-9]+' | tr '\\0' '\\n'",
				"path": "/usr/local/",
				"downloadUrl": "https://adoptopenjdk.net/"
			}
		}
	}
}
