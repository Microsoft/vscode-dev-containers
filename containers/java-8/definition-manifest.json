{
	"definitionVersion": "0.201.4",
	"build": {
		"latest": false,
		"rootDistro": "debian",
		"parent": "java",
		"tags": [
			"java:${VERSION}-8"
		]
	},
	"dependencies": {
		"image": "openjdk:8-jdk-buster",
		"imageLink": "https://hub.docker.com/_/java",
		"apt": [{
			"cgIgnore": false,
			"name": "yarn"
		}],
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh",
			"nvm": "/usr/local/share/nvm"
		},
		"other": {
			"SDKMAN!": {
				"versionCommand": ". /usr/local/sdkman/bin/sdkman-init.sh && sdk version | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+[+,0-9,a-z]*' | tr -d '[:cntrl:]'",
				"downloadUrl": "https://github.com/sdkman/sdkman-cli"
			},
			"AdoptOpenJDK": {
				"versionCommand": "java -version 2>&1 | grep -oE -m 1 '.*build\\s[^\\)]+' | sed -n '/.*build\\s/s///p'",
				"downloadUrl": "https://adoptopenjdk.net/"
			}
		}
	}
}
